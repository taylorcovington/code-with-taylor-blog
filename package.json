    "@remix-run/eslint-config": "^1.3.4",
    "@remix-run/serve": "^1.3.4",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
